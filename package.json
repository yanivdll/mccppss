{
  "devDependencies": {
    "sass": "^1.57.1"
  },
  "scripts": {
    "build": "sass --watch scss/scss:/css",
    "sass": "sass src/scss:public",
    "sass:dev": "sass --watch --embed-source-map scss/style.scss:style.css",
    "sass:prod": "sass --no-source-map --style compressed src/scss:public/css"
  }
}